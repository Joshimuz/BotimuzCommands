<commands version="1">
	<command>
		<name>Why no Radio</name>
		<maxWords>10</maxWords>
		<requiredwords>
			<group>
				<word>why</word>
				<word wholeword="true">y</word>
				<word>where</word>
				<word>botimuz</word>
				<word wholeword="true">bot</word>
			</group>
			<group>
				<word wholeword="true">no</word>
				<word>don</word> <!-- This looks silly but makes it work with don't and dont, and sometimes people use the wrong ' in don't -->
