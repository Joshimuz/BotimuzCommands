<commands version="1">
    <command>
        <name>Colours meaning</name>
        <requiredwords>
            <group>
                <word wholeword="true">bot</word>
                <word>botimuz</word>
                <word>what</word>
                <word wholeword="true">why</word>
            </group>
            <group>
                <word>color</word>
                <word>colour</word>
                <word>blue</word>
                <word>yellow</word>
                <word>brown</word>
                <word wholeword="true">red</word>
                <word>pink</word>
                <word>green</word>
            </group>
            <group>
                <word wholeword="true">why</word>
                <word>sheet</word>
                <word>mean</word>
                <word>card</word>
                <word>board</word>
                <word>square</word>
                <word>grid</word>
                <word>table</word>
                <word>chart</word>
                <word>matrix</word>
                <word>overlay</word>
                <word>5x5</word>
            </group>
        </requiredwords>
        <unwantedwords>
            <group>
                <word>wool</word>
                <word>flower</word>
                <word>dye</word>
                <word>terracotta</word>
                <word>concrete</word>
                <word>bed</word>
                <word>banner</word>
                <word>glass</word>
                <word>armor</word>
                <word>armour</word>
                <word>sheep</word>
            </group>
        </unwantedwords>
        <responses>
            <response>Players have different ways of marking their sheet. Josh's system is mostly: Grey = Unmarked; Blue = To be done; Red = Failed; Green = Completed! The other colours are situational.</response>
        </responses>
    </command>
    <command>
