    <command>
		<name>Can I Join?</name>
		<requiredwords>
			<group>
				<word>what</word>
				<word>which</word>
				<word>is this</word>
				<word wholeword="true">can</word>
				<word>botimuz</word>
				<word wholeword="true">bot</word>
				<word>open</word>
			</group>
			<group>
				<word>join</word>
				<word wholeword="true">server</word>
				<word>play with</word>
				<word>connect</word>
				<word>realms</word>
			</group>
		</requiredwords>
