	<command>
		<name>What car</name>
		<requiredwords>
			<group>
				<word wholeword="true">bot</word>
				<word>botimuz</word>
				<word wholeword="true">what</word>
			</group>
			<group>
				<word wholeword="true">car</word>
			</group>
		</requiredwords>
		<responses>
			<response>PROLaps is driving a GTP, the fastest class (specifically a Cadillac V-Series.R)</response>
		</responses>
	</command>

	<command>
		<name>When Josh Drive?</name>
		<requiredwords>
			<group>
				<word wholeword="true">bot</word>
				<word>botimuz</word>
				<word>when</word>
				<word wholeword="true">how</word>
