	<command>
		<name>Replays</name>
		<maxWords>15</maxWords>
		<requiredwords>
			<group>
				<word>why</word>
				<word>what</word>
				<word wholeword="true">bot</word>
				<word>botimuz</word>
			</group>
			<group>
				<word wholeword="true">replay</word>
			</group>
		</requiredwords>
		<responses>
			<response>Josh uses the replays for two reasons, for making fade out shorter (only saves time with short replays) and for loading the vehicles in the replay (PCJ in Los Santos, Sultan/ZR/Elegy after Badlands)</response>
		</responses>
	</command>
