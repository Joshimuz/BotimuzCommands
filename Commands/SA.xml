	<command>
		<name>No Major Glitches</name>
		<requiredwords>
			<group>
				<word>where</word>
				<word>what</word>
				<word wholeword="true">bot</word>
				<word>mean</word>
				<word>botimuz</word>
				<word wholeword="true">stands</word>
			</group>
			<group>
				<word wholeword="true">nmg</word>
				<word>major glitch</word>
			</group>
		</requiredwords>
		<unwantedwords>
			<group>
				<word>like</word>
				<word>prefer</word>
				<word>wr</word>
				<word>world</word>
				<word>record</word>
			</group>
		</unwantedwords>
		<responses>
			<response>NMG (No Major Glitches) is a version of Any% that forbids OnMission flag manipulation (Duping, 0 Star) and any other "big" glitches that get found in the future</response>
		</responses>
	</command>
	<command>
		<name>No AJS</name>
		<requiredwords>
			<group>
				<word>what</word>
				<word wholeword="true">bot</word>
				<word>where</word>
				<word>mean</word>
				<word>botimuz</word>
				<word>translate</word>
			</group>
			<group>
				<word>ajs</word>
				<word>asj</word>
				<word>jump in script</word>
			</group>
		</requiredwords>
		<unwantedwords>
			<group>
				<word>like</word>
				<word>prefer</word>
				<word wholeword="true">wr</word>
				<word>world</word>
				<word>record</word>
			</group>
		</unwantedwords>
		<responses>
			<response>No AJS (No Arbitrary Jump in Script) forbids manipulating mission script jumping, which is used in normal Any% to skip the whole game. (Not called ASJ, which sounds like All Stunt Jumps.)</response>
		</responses>
	</command>
	<command>
		<name>How many hours</name>
		<maxWords>15</maxWords>
		<requiredwords>
			<group>
				<word wholeword="true">how</word>
			</group>
			<group>
				<word wholeword="true">long</word>
				<word wholeword="true">many</word>
				<word wholeword="true">much</word>
			</group>
			<group>
				<word wholeword="true">played</word>
				<word wholeword="true">hours</word>
			</group>
		</requiredwords>
		<unwantedWords>
			<group>
				<word>sleep</word>
			</group>
		</unwantedWords>
		<responses data0="randomNumber" range0="5000-19999">
			<response>Josh has played GTA:SA for [0] hours.</response>
		</responses>
	</command>
	<command>
		<name>SubsOnly</name>
		<responses forward="false" usename="false">
			<response>/subscribers</response>
		</responses>
		<dataTrigger type="onLoad"></dataTrigger>
	</command>
	<command>
		<name>SubsOnlyOff</name>
		<responses forward="false" usename="false">
			<response>/subscribersoff</response>
		</responses>
		<dataTrigger type="onUnload"></dataTrigger>
	</command>
	<command>
		<name>I left why reset</name>
		<maxWords>20</maxWords>
		<requiredWords>
			<group>
				<word>gone</word>
				<word>afk</word>
				<word>left</word>
				<word>away</word>
				<word>looking</word>
				<word>went to</word>
				<word>head out</word>
				<word>i was</word>
				<word>been out</word>
			</group>
			<group>
				<word>why</word>
				<word>what</word>
				<word>how</word>
				<word wholeword="true">bot</word>
				<word>botimuz</word>
			</group>
			<group>
				<word>reset</word>
				<word>restart</word>
				<word>end</word>
				<word>die</word>
				<word>happen</word>
				<word>over</word>
			</group>
		</requiredWords>
		<unwantedwords>
			<group>
				<word>leftover</word>
			</group>
		</unwantedwords>
		<responses>
			<response>While you were eating for 2 minutes Josh reset because a plane crashed.</response>
			<response>While you were walking your dog for 10 minutes Josh reset because he got busted.</response>
			<response>While you were on the toilet for 3 minutes Josh reset because he failed a mission.</response>
			<response>While you were looking away for 2 seconds Josh reset because the game crashed.</response>
			<response>While you weren't enjoying Josh's content for too long Josh reset because he needed the clip for highlights.</response>
			<response>While you were petting a cat for 4 minutes Josh reset because he fell into water.</response>
			<response>While you were answering your door for 5 seconds Josh reset because Sweet died.</response>
			<response>While you were getting food for 30 seconds Josh reset because he killed Ryder.</response>
			<response>While you were getting a drink for 20 seconds Josh reset because he wanted to.</response>
			<response>While you were checking social media for 3 seconds Josh reset because his parachute failed.</response>
		</responses>
	</command>
